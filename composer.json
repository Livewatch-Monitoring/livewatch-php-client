{
    "name": "livewatch-monitoring/livewatch-php-client",
    "version": "1.0.5",
    "description": "Livewatch Server Monitoring API",
    "keywords": [
        "livewatch",
        "server monitoring",
        "server-monitoring",
        "server",
        "monitoring",
        "ping test",
        "uptime test",
        "server monitor"
    ],
    "homepage": "https://www.livewatch.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Livewatch",
            "homepage": "https://www.livewatch.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "LivewatchApi\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "LivewatchApi\\Test\\" : "test/" }
    }
}
